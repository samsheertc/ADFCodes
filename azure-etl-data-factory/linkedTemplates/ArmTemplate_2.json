					"SourceDirectory": {
						"type": "string",
						"defaultValue": "Input"
					},
					"LoggingPath": {
						"type": "string",
						"defaultValue": "azurecontainer/logs"
					}
				},
				"annotations": []
			},
			"dependsOn": []
