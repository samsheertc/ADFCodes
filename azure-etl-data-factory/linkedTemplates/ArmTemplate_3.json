{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azure-etl-data-factory"
		},
		"trigger1_properties_pipeline_parameters_pl_v_DBName": {
			"type": "string",
			"defaultValue": "ENTDWDB"
		},
		"trigger1_properties_pipeline_parameters_pl_v_TableName": {
			"type": "string",
			"defaultValue": "PipeLineExecution"
		},
		"trigger1_properties_pipeline_parameters_pl_v_Blob_Container": {
			"type": "string",
			"defaultValue": "azurecontainer"
		},
		"trigger1_properties_pipeline_parameters_pl_v_Blob_Directory": {
			"type": "string",
			"defaultValue": "Input"
		},
		"trigger1_properties_pipeline_parameters_pl_v_Blob_File": {
			"type": "string",
			"defaultValue": "employee.json"
		},
		"trigger1_properties_pipeline_parameters_windowStartTime": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"trigger1_properties_pipeline_parameters_windowEndTime": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"trigger1_properties_pipeline_parameters_scheduledTime": {
			"type": "string",
			"defaultValue": "@trigger().scheduledTime"
		},
		"trigger1_properties_pipeline_parameters_startTime": {
			"type": "string",
			"defaultValue": "@trigger().startTime"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/trigger1')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipeline": {
					"pipelineReference": {
						"referenceName": "pipeline_json_to_sqldb_logging",
						"type": "PipelineReference"
					},
					"parameters": {
						"pl_v_DBName": "[parameters('trigger1_properties_pipeline_parameters_pl_v_DBName')]",
						"pl_v_TableName": "[parameters('trigger1_properties_pipeline_parameters_pl_v_TableName')]",
						"pl_v_Blob_Container": "[parameters('trigger1_properties_pipeline_parameters_pl_v_Blob_Container')]",
						"pl_v_Blob_Directory": "[parameters('trigger1_properties_pipeline_parameters_pl_v_Blob_Directory')]",
						"pl_v_Blob_File": "[parameters('trigger1_properties_pipeline_parameters_pl_v_Blob_File')]",
						"windowStartTime": "[parameters('trigger1_properties_pipeline_parameters_windowStartTime')]",
						"windowEndTime": "[parameters('trigger1_properties_pipeline_parameters_windowEndTime')]",
						"scheduledTime": "[parameters('trigger1_properties_pipeline_parameters_scheduledTime')]",
						"startTime": "[parameters('trigger1_properties_pipeline_parameters_startTime')]"
					}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Minute",
					"interval": 5,
					"startTime": "2022-04-06T01:15:00Z",
					"delay": "00:00:00",
					"maxConcurrency": 50,
					"retryPolicy": {
						"intervalInSeconds": 30
					},
					"dependsOn": []
				}
			},
			"dependsOn": []
		}
	]
}